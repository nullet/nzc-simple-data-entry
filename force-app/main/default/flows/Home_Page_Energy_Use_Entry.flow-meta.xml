            <assignToReference>newSAEU.FuelType</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>fuelTypePicklist</elementReference>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>newSAEU.Name</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>newSAEU_recordName</elementReference>
            </value>
