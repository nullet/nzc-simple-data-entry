        <object xsi:nil="true"/>
        <picklistField>FuelType</picklistField>
        <picklistObject>StnryAssetEnrgyUse</picklistObject>
    </dynamicChoiceSets>
    <dynamicChoiceSets>
        <name>StnryAssetEnvSrc_picklist</name>
        <dataType>String</dataType>
        <displayField>Name</displayField>
        <filterLogic>and</filterLogic>
        <filters>
            <field>OwnerId</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>$User.Id</elementReference>
            </value>
        </filters>
        <object>StnryAssetEnvrSrc</object>
        <outputAssignments>
            <assignToReference>newSAEU.StnryAssetEnvrSrcId</assignToReference>
            <field>Id</field>
        </outputAssignments>
        <sortField>LastViewedDate</sortField>
        <sortOrder>Desc</sortOrder>
        <valueField>Name</valueField>
    </dynamicChoiceSets>
    <formulas>
        <name>newSAEU_recordName</name>
        <dataType>String</dataType>
        <expression>{!newSAEU.StnryAssetEnvrSrc.Name} + &quot;-&quot; +  {!Fuel_Type} + &quot;-&quot;+CASE(MONTH( 
{!newSAEU.StartDate}),1,&quot;Jan&quot;,2,&quot;Feb&quot;,3, &quot;Mar&quot;,4, &quot;Apr&quot;,5, &quot;May&quot;,6, &quot;Jun&quot;,7, &quot;Jul&quot;,8, &quot;Aug&quot;,9,&quot;Sep&quot;,10, &quot;Oct&quot;,11,&quot;Nov&quot;,&quot;Dec&quot;)+ &quot;-&quot; + TEXT(YEAR({!newSAEU.StartDate}))</expression>
    </formulas>
