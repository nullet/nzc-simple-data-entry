    <apiVersion>55.0</apiVersion>
    <assignments>
        <name>Assign_values_to_new_SAEU</name>
        <label>Assign values to new SAEU</label>
        <locationX>176</locationX>
        <locationY>278</locationY>
        <assignmentItems>
            <assignToReference>newSAEU.StartDate</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>Start_Date</elementReference>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>newSAEU.EndDate</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>End_Date</elementReference>
            </value>
        </assignmentItems>
        <assignmentItems>
            <assignToReference>newSAEU.FuelConsumptionUnit</assignToReference>
            <operator>Assign</operator>
